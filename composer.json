{
    "name": "zipmegabyte/blogdown",
    "description": "Simple Laravel blog package using markdown",
    "authors": [
        {
            "name": "Leonardo Ribeiro",
            "email": "leonardotsr@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "5.*",
        "michelf/php-markdown": "^1.7",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/laravel": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Swiftmade\\Blogdown\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/IntegrationTestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swiftmade\\Blogdown\\Providers\\BlogdownProvider"
            ]
        }
    }
}
